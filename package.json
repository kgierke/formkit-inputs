    "build": "rollup --config",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kgierke/formkit-inputs.git"
  },
  "keywords": [
    "formkit",
    "inputs",
    "library"
  ],
