  "bugs": {
    "url": "https://github.com/kgierke/formkit-inputs/issues"
  },
  "homepage": "https://github.com/kgierke/formkit-inputs#readme",
  "peerDependencies": {
    "@formkit/core": "1.0.0-beta.6",
    "@formkit/inputs": "1.0.0-beta.6"
  },
  "dependencies": {
    "nanoid": "3.3.1"
  },
  "devDependencies": {
    "@formkit/themes": "1.0.0-beta.6",
    "@formkit/vue": "1.0.0-beta.6",
    "@rollup/plugin-typescript": "8.3.1",
    "cssnano": "5.1.4",
    "rimraf": "3.0.2",
    "rollup": "2.70.1",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-dts": "4.2.0",
    "rollup-plugin-postcss": "4.0.2",
    "typescript": "4.6.2",
